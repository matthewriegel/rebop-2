{
  "name": "rebop",
  "version": "1.0.0",
  "description": "A ball game",
  "main": "index.js",
  "dependencies": {
    "@types/jest": "^23.3.5",
    "@types/lodash": "^4.14.117",
    "@types/react": "^16.4.16",
    "@zeit/next-typescript": "^1.1.1",
    "husky": "^1.1.2",
    "jest": "^23.6.0",
    "lodash": "^4.17.11",
    "next": "^7.0.1",
    "phaser": "^3.14.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2"
  },
  "devDependencies": {},
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm test"
    }
  },
  "jest": {
    "testMatch": [
      "<rootDir>/**/__tests__/**/*.(j|t)s",
      "<rootDir>/**/?(*.)(spec|test).(j|t)s?(x)"
    ],
    "moduleFileExtensions": [
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "web.js",
      "js",
      "web.jsx",
      "jsx",
      "json",
      "node",
      "mjs"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|ts|tsx)$"
    ]
  },
  "author": "Hugh Riegel",
  "license": "ISC"
}
